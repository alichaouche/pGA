///**************************************************************************************
int existanceDeSolution(int indexNewSolution, partitionSCBE* populationSCBE )
{

    int i=0,j,Exist = 0,theSameElement;

    while(i<indexNewSolution && Exist == 0)
    {
        theSameElement = 0;
        for(j=0; j<nbrParties-1; j++)
        {
            if(mpz_cmp((populationSCBE+i)->genotypeEntier[j] ,(populationSCBE+indexNewSolution)->genotypeEntier[j]) ==0)
            {
                theSameElement++;
            }
        }
        if(theSameElement==nbrParties-1)
        {
            Exist = 1;
            break;
        }
        i++;
    }
    return Exist;
}
///**************************************************************************************
void genererLesSolutionBinaireSCBE(partitionSCBE *populationSCBE)
{
    int i,j,k,cocyclesSize = nbrParties*nbrNoeuds;
    mpz_t tmp; mpz_init2(tmp,1000); ///mpz_init2(tmp,99999999999999999999999999999);
    mpz_t binaryTmp; mpz_init2(binaryTmp,1000); ///mpz_init2(binaryTmp,99999999999999999999999999999);

    for(i=0;i<taillePopulation;i++){
        if((populationSCBE+i)->genotype == NULL){
            (populationSCBE+i)->genotype = (int*)malloc(nbrArretes*sizeof(int));
            if((populationSCBE+i)->genotype == NULL){printf("Memory allocation for genotypeSCBE failled \n"); exit(EXIT_FAILURE);}
        }
    }

    for(i=0;i<taillePopulation;i++){
        if((populationSCBE+i)->cocycles == NULL){
            (populationSCBE+i)->cocycles = (int*)malloc(cocyclesSize*sizeof(int));
            if((populationSCBE+i)->cocycles == NULL){printf("Memory allocation for cocyclesSCBE failled\n");exit(EXIT_FAILURE);}
        }
    }



    for(i=0; i<taillePopulation; i++)
    {
        for(j=0; j<nbrParties-1; j++)
        {
            mpz_set(tmp,(populationSCBE+i)->genotypeEntier[j]);
            for(k=0; k<nbrNoeuds-1; k++)
            {
                /**
                    nbrNoeuds-2-k : on la taille de chaque partie du cocyle = nbrNoeuds-1
                    ça veut dire de 0-->nbrNoeuds-2, et pour commencer à partir de la droite
                    nbrNoeuds-2-k ou k représente l'indice de la boucle.
                */
                mpz_mod_ui(binaryTmp,tmp,2);
                (populationSCBE+i)->cocycles[(j*(nbrNoeuds-1))+k] = mpz_get_ui(binaryTmp);
                mpz_fdiv_q_ui(tmp,tmp,2);
            }
        }
    }
}

///======================================================================================
/// the main programm
void cutBasedEncodingWithoutRedandancySCBE(int nbrGeneration,FILE *outputFileSCBE,FILE *outputFilePopSCBE,FILE *outputOptimalSolutionFileSCBE,partitionSCBE *populationSCBE1,partitionSCBE *populationSCBE2, partitionSCBE *solutionDominante)
{
    clock_t t1,t2;
    double temps;
    int iteration=1,bestSolutionIteration,ES= 0;

    t1=clock();

    generatePopulationWithoutRedandancySCBE(populationSCBE1);
    ///calculerGenotypeSCBE(populationSCBE1); l'appel à cette fonction est dans la fonction generatePopulationWithoutRedundancy
    getPartitionFromSolutionSCBE(populationSCBE1);
    checkContrainstAndFitnessPenalizationSCBE(populationSCBE1);
    calculCoutCoupeEtFitnessSCBE(populationSCBE1);
#if writingPopulationInFile
    writeSolutionInFileSCBE(populationSCBE1,outputFilePopSCBE,iteration);
#endif // writingPopulationInFile

    ///initialisation de la solution dominante
    bestSolution=findTheBestSolutionSCBE(populationSCBE1);
    *solutionDominante=*(populationSCBE1+bestSolution);
    bestSolutionIteration =1;
#if writingPopulationInFile
    writeBestSolutionInFileSCBE(solutionDominante,outputFileSCBE,iteration);
#endif // writingPopulationInFile
    nbrApparition=1;

    for(iteration =2 ; iteration <= nbrGeneration; iteration++){

        naturalSelectionSCBE(populationSCBE1,populationSCBE2);
        crossOverSCBE(populationSCBE2, populationSCBE1);
        mutationSCBE(populationSCBE1);
        ///************************************************
        genererLesSolutionBinaireSCBE(populationSCBE1);
        ///************************************************
        calculerGenotypeSCBE(populationSCBE1);
        getPartitionFromSolutionSCBE(populationSCBE1);
        checkContrainstAndFitnessPenalizationSCBE(populationSCBE1);
        calculCoutCoupeEtFitnessSCBE(populationSCBE1);

#if writingPopulationInFile
        writeSolutionInFileSCBE(populationSCBE1,outputFilePopSCBE,iteration);
#endif // writingPopulationInFile

        bestSolution=findTheBestSolutionSCBE(populationSCBE1);
#if writingPopulationInFile
        writeBestSolutionInFileSCBE((populationSCBE1+bestSolution),outputFileSCBE,iteration);
#endif // writingPopulationInFile
        if((populationSCBE1+bestSolution)->coutCoupeNormalise > solutionDominante->coutCoupeNormalise)
        {
            *(solutionDominante) = *(populationSCBE1+bestSolution);
            nbrApparition=1;
            bestSolutionIteration = iteration;
        }
        else
        {
            nbrApparition++;
        }
    }
    t2=clock();
    ///displayTheBestSolutionSCBE(solutionDominante);

    temps = (double)(t2-t1)/CLOCKS_PER_SEC;

    if(bestSolutionIteration>=2){
        ES = ((taillePopulation-tauxReproduction)*(bestSolutionIteration-2))+(taillePopulation+ solutionDominante->id -tauxReproduction+1);
    }
    else {
        ES = solutionDominante->id +1;
    }

    writeOptimalSolutionInFileSCBE(solutionDominante,outputOptimalSolutionFileSCBE,nbrRun,bestSolutionIteration,temps,ES);
    printf("le temps d execution est %lf \n", temps);

    fprintf(outputFileSCBE,"\n\n");
    writeBestSolutionInFileSCBE(solutionDominante,outputFileSCBE,bestSolutionIteration);
    fprintf(outputFileSCBE,"\n\nLe nombre d'apparition de la meilleur solution est  = %d\n",nbrApparition);
    fprintf(outputFileSCBE,"\n\n==============================================================================\n");
    fprintf(outputFilePopSCBE,"\n\n==============================================================================\n");



}

///**************************************************************************************
void checkContrainstAndFitnessPenalizationSCBE(partitionSCBE *populationSCBE)
{

    int i,j,k;

    for(i=0; i<taillePopulation; i++)
    {
        (populationSCBE+i)->nbrCluster=0; /// le nombre de cluster doit etre initialisé à 1 pour prendre en consédiration le premier cluster

        ///initialisation de vecteur des contraintes et de la variable de contraintes violées
        (populationSCBE+i)->contrainteViole=0;
        for(j=0; j<4; j++)
        {
            (populationSCBE+i)->constraintVector[j]=0;
        }
        ///**********************************************************************************


        ///trier le tableau résulant
        for(j=0; j<nbrNoeuds; j++)
        {
            (populationSCBE+i)->clustersSize[j] =0;
            for(k=0; k<nbrNoeuds; k++)
            {
                if ((populationSCBE+i)->phenotype[k] == j)
                {
                    (populationSCBE+i)->clustersSize[j]++;
                }
            }
            if ((populationSCBE+i)->clustersSize[j] !=0)
            {
                (populationSCBE+i)->nbrCluster++;
            }
        }

        if((populationSCBE+i)->nbrCluster > max_clusters || (populationSCBE+i)->nbrCluster < min_clusters)
        {
            (populationSCBE+i)->constraintVector[0] = 1; /// le nombre de cluster n'est pas valide
            (populationSCBE+i)->contrainteViole++;
        }

        for(j=0; j<nbrNoeuds; j++)
        {
            if((populationSCBE+i)->clustersSize[j]!=0)
            {
                if((populationSCBE+i)->clustersSize[j]>max_sizeCluster || (populationSCBE+i)->clustersSize[j]< min_sizeCluster)
                {
                    (populationSCBE+i)->constraintVector[1]++;

                }
            }

        }

        if((populationSCBE+i)->constraintVector[1] != 0)
        {
            (populationSCBE+i)->contrainteViole++;
        }
    }
    if(nbrCohabitationConstraintes!=0 || nbrNonCohabitationConstraintes !=0)
    {
        checkCohabitationAndNonCohabitationConstrainteSCBE(populationSCBE);
    }

}

///**************************************************************************************
void checkCohabitationAndNonCohabitationConstrainteSCBE(partitionSCBE *populationSCBE)
{

    int i,j,noeud1,noeud2;
    for(i=0; i<taillePopulation; i++)
    {
        /// Cette tache est déjà réalisé au niveau de la fonction checkCohabitationAndNonCohabitationConstrainteBE
		/**
		(populationSCBE+i)->constraintVector[2]=0;
        (populationSCBE+i)->constraintVector[3]=0;
		*/
        if(nbrCohabitationConstraintes!=0)
        {
            for(j=0; j<nbrCohabitationConstraintes; j++)
            {
                noeud1=cohabitationConstraintes[j].nouedDepart;
                noeud2=cohabitationConstraintes[j].nouedArrive;

                if((populationSCBE+i)->phenotype[noeud1]!= (populationSCBE+i)->phenotype[noeud2])
                {
                    (populationSCBE+i)->constraintVector[2]++;
                }
            }
            /// cette instruction permet d'avoir le nombre de contraintes violées
            if((populationSCBE+i)->constraintVector[2]!=0)
            {
                (populationSCBE+i)->contrainteViole++;
            }
        }


        if(nbrNonCohabitationConstraintes!=0)
        {
            for(j=0; j<nbrNonCohabitationConstraintes; j++)
            {
                noeud1=nonCohabitationConstraintes[j].nouedDepart;
                noeud2=nonCohabitationConstraintes[j].nouedArrive;
                if((populationSCBE+i)->phenotype[noeud1]== (populationSCBE+i)->phenotype[noeud2])
                {
                    (populationSCBE+i)->constraintVector[3]++;
                }
            }
            /// cette instruction permet d'avoir le nombre de contraintes violées
            if((populationSCBE+i)->constraintVector[3]!=0)
            {
                (populationSCBE+i)->contrainteViole++;
            }
        }


    }

}

