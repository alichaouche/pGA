///**************************************************************************************
void getPartitionFromSolutionSCBE(partitionSCBE *populationSCBE)
{

    int i,j,k,nd,na,numeroClustre; /// nd = noued de départ , na = noued d'arrivé

    for(i=0; i<taillePopulation; i++)
    {
        /// initialisation des phénotype = partition par les numero des noeuds correspondant
        for(j=0; j<nbrNoeuds; j++)
        {
            (populationSCBE+i)->phenotype[j] = j;
        }

        /// détermination de la parition
        ///for(j=0; j<nbrArretes; j++)
        for(j=nbrArretes-1; j>=0; j--)
        {
            if((populationSCBE+i)->genotype[j]==0)
            {
                na = edgeVector[j].nouedArrive;
                nd = edgeVector[j].nouedDepart;

                if((populationSCBE+i)->phenotype[na]!= na) /// il a été modifier auparavant
                {

                    if((populationSCBE+i)->phenotype[na] < (populationSCBE+i)->phenotype[nd])
                    {
                        ///dans le cas ou le noued d'arrivé est inférieur au noued d'arrivé ==> toutes les occurence du noued
                        /// de départ prendront ceux du noeud d'arrivé
                        numeroClustre = (populationSCBE+i)->phenotype[nd];
                        for(k=0; k<nbrNoeuds; k++)
                        {
                            if((populationSCBE+i)->phenotype[k] == numeroClustre)
                            {
                                (populationSCBE+i)->phenotype[k] = (populationSCBE+i)->phenotype[na];
                            }
                        }
                    }
                    else if((populationSCBE+i)->phenotype[na] > (populationSCBE+i)->phenotype[nd])
                    {
                        numeroClustre = (populationSCBE+i)->phenotype[na];
                        for(k=0; k<nbrNoeuds; k++)
                        {
                            if((populationSCBE+i)->phenotype[k] == numeroClustre)
                            {
                                (populationSCBE+i)->phenotype[k] = (populationSCBE+i)->phenotype[nd];
                            }
                        }
                    }
                }
                else
                {
                    (populationSCBE+i)->phenotype[na] = (populationSCBE+i)->phenotype[nd];
                }
            }
        }
    }
}
///************************************************************************************************************
/// calculer le cout de coupe de chaque partition ainsi que son fitness
///***************************************************************************************
/// on va appliquer la formule suivante pour le calcule des cout de coupe
/// T'(C) = B - T(C)            <=> la minimisation devient une maximisation
/// Z(C)=T'(C)+(u-v(C)) ×B      <=>  calculer la nouvelle fitness par rapport aux contrainte
/// T'(C) : cout de coupe (maximiser les flux INTRA), u ; nombre de contrainte totale
/// v(C): nombre de contrainte violé,  B: la somme totale des flux
///***************************************************************************************

void calculCoutCoupeEtFitnessSCBE(partitionSCBE* populationSCBE)
{
    int i,j,k,sommeTotalCoutCoupe = 0;

    for(i=0; i<taillePopulation; i++) /// parcourir tous les individus de la population
    {
        (populationSCBE+i)->coutCoupe = 0; /// réinitialisation de la valriable après chaque itération (individus de la population)
        for(j=0; j<nbrNoeuds-1; j++)
        {
            for(k=j+1; k<nbrNoeuds; k++)
            {
                if((populationSCBE+i)->phenotype[j] == (populationSCBE+i)->phenotype[k] && fluxMatrix[j][k] > 0)
                {
                    (populationSCBE+i)->coutCoupe = (populationSCBE+i)->coutCoupe +  fluxMatrix[j][k];
                }
            }
        }
        (populationSCBE+i)->coutCoupeNormalise = (populationSCBE+i)->coutCoupe + ((nbr_constraint - (populationSCBE+i)->contrainteViole)*sommeTotalFlux);
        sommeTotalCoutCoupe = sommeTotalCoutCoupe + (populationSCBE+i)->coutCoupeNormalise;
    }

#if scaling

    FILE *file;
    file = fopen("D:/WorkingFolderAgProgramming/benchmark/sortedCutBasedEncoding/expectedValueSCBE.txt","w");
    ///======================================================================================================
    /// Scaling Fintnss : sigma scaling Melanie mitchelle référence Goldberg | le 26/11/2015
    ///pour régler le problème des valeurs négatives des expected values ==> utiliser sigma truncation de goldberg
    /// g(f) = f + (moyenne - c * sigma )
    /// 1 <= c <= 5, généralement initilisé à 2
    ///======================================================================================================
    float varianceCoutDeCoupeNormalise = 0, moyenneCoutDeCoupeNormalise, sigma, sommeTotalOfExpectdValues = 0;
    int c= 2;

    moyenneCoutDeCoupeNormalise = sommeTotalCoutCoupe / taillePopulation;
    for(i=0; i<taillePopulation; i++)
    {
        varianceCoutDeCoupeNormalise =
            varianceCoutDeCoupeNormalise + pow(((populationSCBE+i)->coutCoupeNormalise - moyenneCoutDeCoupeNormalise),2);
    }
    varianceCoutDeCoupeNormalise = varianceCoutDeCoupeNormalise / taillePopulation;
    sigma = sqrt(varianceCoutDeCoupeNormalise);
    /// tester la valeur ajouté
    for(i=0; i<taillePopulation; i++)
    {

        /// calcul de sigma truncation
        (populationSCBE+i)->expectedValue = (populationSCBE+i)->coutCoupeNormalise + (moyenneCoutDeCoupeNormalise - c*sigma);

        /**
                    if(iteration == 1){
                        fprintf(file,"%d \t %d \t %0.2f \n",i,(populationSCBE+i)->coutCoupeNormalise,(populationSCBE+i)->expectedValue);
                    }
        */

        sommeTotalOfExpectdValues = sommeTotalOfExpectdValues + (populationSCBE+i)->expectedValue;
    }

    /// calcule du fitness
    /// le 26/11/2015 : calcul du fitness des solution avec leur expectedValues
    for(i=0; i<taillePopulation ; i++)
    {
        (populationSCBE+i)->fitness = (float)((populationSCBE+i)->expectedValue)/(float)(sommeTotalOfExpectdValues);
    }

#else
    for(i=0; i<taillePopulation ; i++)
    {
        (populationSCBE+i)->fitness = (float)((populationSCBE+i)->coutCoupeNormalise)/(float)(sommeTotalCoutCoupe);
    }
#endif// scaling


}

///**************************************************************************************
/// calculer le cout de coupe de chacune des solution dans la population
void calculCoutCoupeEtFitnessWithFlowVectorSCBE(partitionSCBE* populationSCBE)
{
#if mouchard
    printf("calculCoutCoupeEtFitness ...\n");
#endif // mouchard
    int i,j,sommeTotalCoutCoupe = 0;
    for(i=0; i<taillePopulation; i++) /// parcourir tous les individus de la population
    {
        (populationSCBE+i)->coutCoupe = 0;

        for(j=0; j<nbrArretes; j++) ///parcourir le vecteurSolution =  solution codée en binaire
        {
            if((populationSCBE+i)->genotype[j]==0)  /// on cherche à maximiser les intraCluster donc les arrêtes 0 != 1
            {
                (populationSCBE+i)->coutCoupe = (populationSCBE+i)->coutCoupe + fluxVector[j];
            }
        }
        sommeTotalCoutCoupe = sommeTotalCoutCoupe + (populationSCBE+i)->coutCoupe;
    }
    /// calcule du fitness
    for(i=0; i<taillePopulation ; i++)
    {
        ///(populationSCBE+i)->fitness = (float)((populationSCBE+i)->coutCoupe)/sommeTotalCoutCoupe;
        (populationSCBE+i)->fitness = (float)((populationSCBE+i)->coutCoupe)/(float)(sommeTotalCoutCoupe);
    }
}


